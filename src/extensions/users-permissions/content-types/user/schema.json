{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "account_status": {
      "type": "enumeration",
      "required": true,
      "default": "PENDING",
      "enum": [
        "APPROVED",
        "CREATE_APPROVED",
        "DENIED",
        "PENDING",
        "REVIEWING",
        "BLOCKED"
      ]
    },
    "business_name": {
      "type": "string",
      "minLength": 3,
      "unique": true
    },
    "business_number": {
      "type": "string",
      "minLength": 3,
      "unique": true
    },
    "business_type": {
      "type": "enumeration",
      "enum": [
        "INSTALLER",
        "RETAILER"
      ]
    },
    "phone": {
      "type": "string"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "user"
    },
    "carts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart.cart",
      "mappedBy": "user"
    },
    "createAccountRequest": {
      "type": "datetime"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "creditCards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::credit-card.credit-card",
      "mappedBy": "user"
    },
    "stripeCustomerID": {
      "type": "string"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order"
    },
    "order": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "user"
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },
    "nickname": {
      "type": "string"
    },
    "confirmed": {
      "type": "boolean"
    },
    "isRegisteredForNews": {
      "type": "boolean"
    },
    "refreshToken": {
      "type": "string"
    },
    "tokenVersion": {
      "type": "string"
    },
    "odoo_user_id": {
      "type": "string"
    },
    "user_level": {
      "type": "enumeration",
      "enum": [
        "DEFAULT",
        "SMALL",
        "MID_SIZE",
        "VIP",
        "WHOLE_SELLER"
      ]
    },
    "name": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::name.name",
      "mappedBy": "user"
    },
    "triggered_notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "trigger_user"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "admin_user"
    }
  }
}
